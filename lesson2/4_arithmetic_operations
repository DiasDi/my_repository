a = 12
b = 5

#Сложение
print("a + b =", a + b)

#Вычитание
print("a - b =", a - b)

#Умножение
print("a * b =", a * b)

#Деление (Возвращает частное от деление a на b)
print("a / b =", a / b)

#Деление (Возвращает неполное частное от деления)
print("a // b =", a // b)

#Остаток от деления (Возвращает остаток от деления)
print("a % b =", a % b)

#Возведение в степень
print("a ** b =", a ** b)